{
  "manifest_version": 3,
  "name": "Full Page Screenshot",
  "version": "1.0",
  "permissions": ["activeTab","scripting", "storage"  
],
"host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "full-page-screenshot-16_x_16.png",
      "48": "full-page-screenshot-48_x_48.png",
      "128": "full-page-screenshot-128_x_128.png"
    }
  },
  "icons": {
    "16": "full-page-screenshot-16_x_16.png",
    "48": "full-page-screenshot-48_x_48.png",
    "128": "full-page-screenshot-128_x_128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
